{
    "name": "jeoip/contracts",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jeoip\\Contracts\\": "src/"
        }
    },
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1"
    },
    "scripts": {
        "test:codestyle": "vendor/bin/php-cs-fixer fix -v --dry-run --stop-on-violation --using-cache=no",
        "test": [
            "@test:codestyle"
        ]
    }
}
